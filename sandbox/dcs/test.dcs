#include number.dci
#include character.dci
#include dynamic.dci

external routine puts dynamic align<character> str;

internal routine foo static<number> align<number> i {
    number++ (subscript i, 0);
    return;
}

internal routine bar static<number> align<number> i, static<number> align<number> out {
    static<number> align<number> j;
    number= (subscript j, 0), (number+ (subscript i, 0), 1);

    number= (subscript out, 0), (subscript j, 0);
    return;
}

routine main static<number> align<number> argc, dynamic align<dynamic align<character> argv, static<number> align<number> out {
    dynamic align<character> str;
    dynamic= str, "Hello, world";
    puts, str;

    number= (subscript out, 0), 0;
    return;
}
